  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coloradix/coloradix.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coloradix/coloradix/issues"
  },
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "@coloradix/colors": "1.2.0"
  },
  "peerDependencies": {
    "tailwindcss": "3.4.1 - 4.0.0"
  },
  "devDependencies": {
    "@coloradix/tsconfig": "undefined"
  }
}
