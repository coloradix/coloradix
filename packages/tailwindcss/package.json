{
  "name": "@coloradix/tailwindcss",
  "version": "2.3.1",
  "description": "Coloradix for tailwindcss",
  "keywords": [
    "@radix-ui/colors",
    "tailwindcss"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
